<?xml version="1.0" encoding="UTF-8"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

	<Description about="urn:mozilla:install-manifest">
		<em:id>tito@tabclick</em:id>
		<em:name>Tab click</em:name>
		<em:version>0.3</em:version>
		<em:description>A click on current tab will focus the previous focused tab.</em:description>
		<em:creator>Tito Bouzout</em:creator>
		<em:homepageURL>https://github.com/titoBouzout/mozilla-tab-click</em:homepageURL>

		<!-- Firefox -->
